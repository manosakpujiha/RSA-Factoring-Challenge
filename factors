#!/usr/bin/env bash
# is a factors challenge


import math

def factors(n):
    result = []
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0:
            result.append(i)
            result.append(n // i)
    return result

with open('file.txt', 'r') as f:
    for line in f:
        n = int(line.strip())
        p, q = factors(n)
        print(f'{n}={p}*{q}')
